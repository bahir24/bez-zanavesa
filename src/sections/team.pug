                  ]                              
                each people in peoples
                  li.people__item
                    .actor
                      .actor__avatar
                        +image(`team/${people.name}.jpg`, 'actor__avatar-pic')
                      .actor__description
                        h4.actor__description-name #{people.name}
                        h5.actor__description-position #{people.position}
                        .actor__description-about
                          p.actor__description-text #{people.description}