.btn {
  // min-width: 120px;
  text-align: center;
  color: #000;
  background-color: #fff;
  font-size: 12px;
  font-weight: 700;
  padding: 10px;
  border: 1px solid #000;
  border-radius: 5px;
  text-decoration: none;

  &--form {
    font-size: 16px;
    font-weight: 700;
    background-color: #000;
    color: #fff;
    border-radius: 0;
    padding: 20px 50px;
    
  }
  &--width {
    width: 40%;
  }
}