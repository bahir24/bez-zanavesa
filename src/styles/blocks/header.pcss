.header {
  background: #000;
  height: 10vh;
  border: none;
}

.container-header {
  display: flex;
  width: 95%;
  height: 100%;
  // align-items: center;
  // justify-content: space-between;
  // font-weight: 700;

  @include tablets {
    justify-content: space-between;
    width: 100%;

  }
}

.header__wrapper {
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: space-between;
  @include tablets {
    display: none;

  }
}